<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
    assembly = "CORE" namespace = "CORE.Entities">
    
    <class name="Transaction" table="transactions">
        <id name="Id" generator="increment">    
            <column name="id"></column>
        </id>
        <property name="UserId">
            <column name="user_id"></column>
        </property>   
          <property name="Exchange">
            <column name="exchange"></column>
        </property> 
          <property name="Deposit">
            <column name="deposit"></column>
        </property> 
          <property name="Withdrawal">
            <column name="withdrawal"></column>
        </property> 
           <property name="CreatedDate">
            <column name="created_date"></column>
        </property>
           <property name="Quantity">
            <column name="quantity"></column>
        </property> 
    </class>   
</hibernate-mapping>
